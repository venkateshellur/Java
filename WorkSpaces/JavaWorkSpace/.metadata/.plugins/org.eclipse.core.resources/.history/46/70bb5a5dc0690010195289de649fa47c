package com.api.virtualThreads;

import java.util.concurrent.TimeUnit;

class SleepingThread implements Runnable{

	@Override
	public void run() {
		try {
			TimeUnit.SECONDS.sleep(1);
		} catch (Exception e) {
			
		}
	}
	
}

