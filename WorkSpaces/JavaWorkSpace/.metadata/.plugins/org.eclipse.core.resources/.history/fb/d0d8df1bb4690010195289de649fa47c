package com.api.TextBlocks;

public class TextBlocksRunner {

	public static void main(String[] args) {
		String str = """
				Line 1
					"Line 2"
				Line 3
				""";

		String textBlock = """
				Line 1 : %s
					Line 2 : %d
						Line3 : %s
				""".formatted(" Venky is ", 24, " Forever");

		System.out.println(str);
		System.out.println(textBlock);
	}

}
